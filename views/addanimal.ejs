                                                                    <label class="labeltest" for="file-ip-1">
                                                                        <span> Drop image here or click to upload.
                                                                        </span>
                                                                    </label>
                                                                    <input type="file" name="an_pic" id="file-ip-1" accept="image/*" onchange="showPreview(event)" required>
                                                                    <div class="preview">
                                                                        <img id="file-ip-1-preview" src="#" alt="img">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xl-4">
                                                        <div class="form-group">
                                                            <span>動物名字:</span>
                                                            <input type="text" name="an_name" class="form-control"
                                                                placeholder="動物名字" required>
                                                        </div>
                                                        <div class="form-group">
                                                            <span>動物品種:</span>
                                                            <input type="text" name="an_variety" class="form-control"
                                                                placeholder="動物品種" required>
                                                        </div>
                                                        <div class="form-group">
                                                            <span>生日:</span>
                                                            <input type="date" name="an_birth" class="form-control"
                                                                placeholder="生日" required>
                                                        </div>
                                                    </div>
                                                    <div class="col-xl-4">

                                                        <div class="form-group">
                                                            <span>體重:</span>
                                                            <input type="text" name="an_size" class="form-control"
                                                                placeholder="體重" required>
                                                        </div>
                                                        <div class="form-group">
                                                            <span>性別:</span>
                                                            <select name="an_sex" class="form-control form-select" required>
                                                                <option value="性別" hidden></option>
                                                                <option>母</option>
                                                                <option>公</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-xl-4">
                                                        <!--   </div>
